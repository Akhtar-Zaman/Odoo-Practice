<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="transient_action_patient_filter" model="ir.actions.act_window">
        <field name="name">transient.action.patient</field>
        <field name="res_model">patient.filter</field>
        <field name="view_mode">form</field>
        <!-- <field name="domain">[()]</field> -->
    </record>

    <record id="transient_form_view" model="ir.ui.view">
        <field name="name">transient.form</field>
        <field name="model">patient.filter</field>
        <field name="arch" type='xml'>
            <form>
                <sheet>
                    <group>
                        <!-- <field name="doc_namee"/> -->
                        <field name="date_from"/>
                        <field name="date_to"/>                       
                    </group>
                    <footer>
                        <!-- <button string="OK" name="%(transient_action_patient_filter)d" type="action" class="oe_highlight"/> -->
                        <button string="Object" name="object" type="object" class="oe_highlight"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>


</odoo>
